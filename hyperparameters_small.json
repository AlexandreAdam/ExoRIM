{
		"steps": 8,
		"pixels": 32,
		"channels": 1,
		"state_size": 16,
		"state_depth": 64,
		"log_likelihood_term": ["visibility"],
		"learning rate": {
            "initial_learning_rate": 1e-3,
            "decay_steps": 10,
            "decay_rate": 0.90
        },
		"Regularizer Amplitude": {
				"kernel": 0.01,
				"bias": 0.01
		},
		"Downsampling Block": [
				{"Conv_Downsample": {
						"kernel_size": [5, 5],
						"filters": 2,
						"strides": [2, 2]
				}}
		],
		"Convolution Block": [
				{"Conv_1": {
						"kernel_size": [3, 3],
						"filters": 16,
						"strides": [1, 1]
				}},
				{"Conv_2": {
						"kernel_size": [3, 3],
						"filters": 32,
						"strides": [1, 1]
				}}
		],
		"Recurrent Block": {
				"GRU_1": {
						"kernel_size": [3, 3],
						"filters": 32
				},
				"GRU_2": {
						"kernel_size": [3, 3],
						"filters": 32
				}
		},
		"Upsampling Block": [
				{"Conv_Fraction_Stride": {
						"kernel_size": [3, 3],
						"filters": 32,
						"strides": [2, 2]
				}}
		],
		"Transposed Convolution Block": [
				{"TConv_1": {
						"kernel_size": [3, 3],
						"filters": 16,
						"strides": [1, 1]
				}},
				{"TConv_2": {
						"kernel_size": [3, 3],
						"filters": 1,
						"strides": [1, 1] 
				}}
		]
}
